{
  "name": "create-tailwind-app",
  "version": "1.0.1",
  "description": "Build the HTML template with tailwind css.",
  "type": "commonjs",
  "scripts": {
  "build:css": "npx tailwindcss -i ./src/styles/style.css -o ./_site/assets/style.css --watch",
  "dev": "npx @11ty/eleventy --serve",
  "build": "npm run build:css && npx @11ty/eleventy"
  },
  "keywords": [
    "HTML template",
    "tailwindcss",
    "tailwind",
    "HTML",
    " css framework"
  ],
  "author": "Rajdeep Singh",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "live-server": "^1.2.2",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2"
  }
}
